<?xml version="1.0" encoding="utf-8"?>
<openbox_menu xmlns="http://openbox.org/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://openbox.org/                 file:///usr/share/openbox/menu.xsd">
    <menu id="root-menu" label="Openbox 3">
        <item label="emulationstation">
            <action name="Execute">
                <execute>emulationstation</execute>
            </action>
        </item>
        <item label="RetroArch Configuration">
            <action name="Execute">
                <execute>retroarch-config</execute>
            </action>
        </item>
        <item label="Terminal emulator">
            <action name="Execute">
                <execute>urxvt</execute>
            </action>
        </item>
        <item label="Chromium">
            <action name="Execute">
                <execute>chromium</execute>
            </action>
        </item>
        <separator/>
        <item label="Exit">
            <action name="Exit"/>
        </item>
    </menu>
</openbox_menu>
